<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.orjujeng.request.mapper.MemberInfoMapper">
 <update id="updateBdAuth" parameterType="String">
 	update PROFILE_API.member_info set auth_of_backend = 'Y'
 	where id = #{memberId}
 </update>
</mapper>